ЗАДАНИЕ 1
m = ['круг', 0.25, 'квадрат', 'треугольник', 15, 'круг', 'овал', '10']
for i in range(len(m)-1, -1, -1):
    if not isinstance(m[i], str):  
        m.remove(m[i])  
print(m) 

ЗАДАНИЕ 2
abc = ['A', 'B', 'C', 'D', 'E', 'F', 'G']
del abc[1:5] 
print(abc

ЗАДАНИЕ 3
numbers = [1, 4]

for i in range(len(numbers)-1):
    diff = numbers[i+1] - numbers[i]
    if diff > 1:
        for j in range(1, diff):
            numbers.insert(i+j, numbers[i] + j)

print(numbers)

ЗАДАНИЕ 4
mass = [14, -6, 3, 11, 6, 8.5, 99, -20, -6, 10, 40, 0.25, -4, 5]
positive_numbers = [x for x in mass if x >= 0]
ascending_order = sorted(positive_numbers)
descending_order = sorted(positive_numbers, reverse=True)
print(ascending_order)
print(descending_order)
